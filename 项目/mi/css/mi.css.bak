

/*********************页面顶部top***************/
.top_box ul li{
	float:left;
}
.top_box a{
	margin-right:10px;
}
.top_box ul li a:hover{
	color:#fff;
}
.top_box b{
	height:10px;
	border-left:1px solid #424242;
	padding:0 10px 0 0; 
}
.top_rt{
	float:right;
	height:40px;
}
.top_rt a:hover{
	color:#fff;
}
.top_rt .shop{
	display:inline-block;
	width:80px;
	background:url(../Image/shop.jpg) no-repeat 10px #424242;
	height:41px;
	line-height:40px;
	padding-left:40px;
	margin-right:0px;
}
.top_rt .shop:hover{
	background:url(../Image/shop_hover.jpg) no-repeat 10px #fff;
	color:#FF6700;
}
.shop_box{
	border:1px solid #E0E0E0;
	border-top:1px solid transparent;
	position:absolute;
	z-index:1;
	top:40px;
	right:338px;
	height:120px;
	line-height:120px;
	width:220px;
	background:#fff;
	font-size:14px;
	text-align:center;
	display:none;
}
/************************导航nav***********************/
.nav_center{
	float:left;
	margin-left:190px;
	height:55px;
}
.nav_menus{
	height:55px;
}
.nav_center ul li{
	float:left;
	height:55px;
	line-height:55px;
	margin:0 10px;	
}
.nav_center ul li a{
	display:inline-block;
	color:#333333;
	font-size:16px;
}
.nav_center ul li a:hover{
	color:#FF6700;
}
.nav_search{
	height:55px;
}
.nav_search input{
	border:1px solid #E0E0E0;
	height:48px;
	width:230px;
	outline:none;
	padding-left:5px;
	float:right;
}
.nav_search input:hover{
	border:1px solid #AFAFAF;
}
.nav_search input:focus{
	border:1px solid #FF6700;
}
.nav_search a{
	float:right;
	border:1px solid #E0E0E0;
	border-left:1px solid transparent;
	width:48px;
	height:50px;
	background:url(../Image/search.jpg) no-repeat 100%;
}
.nav_search a:hover{
	border:1px solid #FF6700;
	background:url(../Image/search_hover.jpg) no-repeat 100%;
}
.nav_img{
	position:absolute;
	z-index:2;
	width:100%;
	height:200px;
	top:75px;
	background:#fff;
	display:none;
	left:0;
}
/***************************banner部分*****************************/
.banner_top{
	height:460px;
	background:url(../Image/banner_01.jpg) no-repeat;
}
.banner_top .banner_lf{
	float:left;
	height:420px;
	width:220px;
	background:#5E5E5E;
	padding:20px 0;
	opacity:0.8;
}
.banner_top .banner_lf li a{
	display:inline-block;
	font-size:14px;
	color:#fff;
	height:42px;
	line-height:42px;
	width:195px;
	padding-left:25px;
	background:url(../Image/banner_arrow.jpg) no-repeat 90%;
}
.banner_top .banner_lf li a:hover{
	background:url(../Image/banner_arrow_hover.jpg) no-repeat 90% #FF6700;
}
.banner_box{
	float:left;
	height:460px;
	min-width:300px;
	background:#fff;
	display:none;
}
.banner_icons{
	position:relative;
	float:right;
	top:415px;
	right:30px;
}
.banner_icons li{
	float:left;
	width:7px;
	height:7px;
	margin:10px 5px;
	border:3px solid #ADADAD;
	border-radius:50%;
	background:#8B8B8B;
}
.banner_icons li.icon_circle{
	background:transparent;
}
.banner_down{
	margin-top:20px;
	height:170px;
}
.banner_down ul{
	float:left;
	width:220px;
	height:170px;
	background:#605750;
	margin-right:1px;
}
.banner_down ul li{
	float:left;
}
.banner_down ul li a{
	display:inline-block;
	width:50px;
	height:70px;
	line-height:70px;
	margin:7px 11px;
}
.banner_down div{
	float:left;
	height:170px;
}
.banner_down div>a{
	display:inline-block;
	margin-left:16px;
}
.banner_star{
	margin:50px 0;
	height:384px;
}
.banner_star>div{
	overflow:hidden;
	margin:20px 0;
}
div.lr_icon{
	float:right;
	width:73px;
	height:22px;
	border:1px solid #DEDEDE;
}
 div.lr_icon a{
	display:inline-block;
	width:34px;
	height:22px;
}
.banner_star div a.star_icon{
	border-left:1px solid #DEDEDE;
}
.banner_star dl{
	float:left;
	width:234px;
	height:339px;
	background:#FAFAFA;
}
.banner_star dl:nth-child(2){
	border-top:1px solid #FF6700;
	margin-right:14px;
}
.banner_star dl:nth-child(3){
	border-top:1px solid #81C451;
	margin-right:14px;
}
.banner_star dl:nth-child(4){
	border-top:1px solid #1F96F4;
	margin-right:14px;
}
.banner_star dl:nth-child(5){
	border-top:1px solid #E43B34;
	margin-right:14px;
}
.banner_star dl:nth-child(6){
	border-top:1px solid #00C3A6;
}

